<h1>Welcome</h1>
<h1>Log in</h1>
<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log in' %>
  </p>
<% end %>

<%= link_to("Register", new_users_path) %>
